<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
	
<hibernate-mapping>

    <class name="kqb.Todaykq" table="gt_kqb_todaykq">
    	<id name="id" type="int" column="id">
    		<generator class="identity"></generator>
    	</id>
		<property name="sczname" column="sczname"/>
    	<property name="jobid" column="jobid"/>
    	<property name="hours" column="hours"/>
    	<property name="userid" column="userid"/>
    	<property name="sizeid" column="sizeid"/>
    	<property name="number" column="number"/>
    	<property name="month" column="month"/>
    	<property name="day" column="day"/>
    	<property name="createDate" column="createDate"/>
    	
    	
	</class>
</hibernate-mapping>